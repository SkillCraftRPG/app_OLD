{
  "AllowedHosts": "*",
  "BearerToken": {
    "LifetimeSeconds": 900,
    "TokenType": "Bearer"
  },
  "Caching": {
    "ActorLifetime": "00:15:00"
  },
  "Cookies": {
    "RefreshToken": {
      "HttpOnly": true,
      "MaxAge": "7.00:00:00",
      "SameSite": "Strict",
      "Secure": true
    },
    "Session": {
      "SameSite": "Strict"
    }
  },
  "Cors": {
    "AllowedMethods": [ "DELETE", "GET", "PATCH", "POST", "PUT" ],
    "AllowedHeaders": [ "Content-Type" ],
    "AllowCredentials": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Portal": {
    "Realm": "skillcraftrpg"
  }
}
